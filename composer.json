{
  "name": "passionweb/open-ai-api",
  "license": "GPL-3.0",
  "description": "Shows the usage of the OpenAI API. Sends an example request with the entered prompt.",
  "type": "typo3-cms-extension",
  "homepage": "https://www.passionweb.de",
  "version": "1.0.0",
  "authors": [
    {
      "name": "Manuel Schnabel",
      "role": "Developer"
    }
  ],
  "require": {
    "typo3/cms-core": "^11.5 | ^12.4"
  },
  "autoload": {
    "psr-4": {
      "Passionweb\\OpenAiApi\\": "Classes/"
    }
  },
  "replace": {
      "typo3-ter/open-ai-api": "self.version"
  },
  "extra": {
    "typo3/cms": {
      "extension-key": "open_ai_api"
    }
  }
}
